<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<cssAttributes>
	<property name="background">
		<attribute name="background-color">background-color</attribute>
<!-- 		<attribute name="background-image">background-image</attribute> -->
		<attribute name="background-position">background-image,background-position</attribute>
		<attribute name="background-repeat">background-image,background-repeat</attribute>
